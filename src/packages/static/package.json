{
  "name": "@cocalc/static",
  "workspaces": [
    "../../smc-util",
    "../../smc-util-node",
    "../../webapp-lib",
    "../../smc-webapp",
    "../cdn"
  ],
  "dependencies": {
    "@cocalc/cdn": "*",
    "webapp-lib": "*"
  },
  "version": "1.13.7",
  "description": "CoCalc's Static Frontend Webapp Assets",
  "main": "./src/index.js",
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-transform-react-jsx"
    ]
  },
  "scripts": {
    "webpack": "NODE_OPTIONS=--max_old_space_size=8000  NODE_ENV=development webpack --progress --color --watch",
    "tsc-webapp": "cd node_modules/smc-webapp && npx tsc --watch",
    "tsc-static": "cd src && npx tsc --watch",
    "webpack-prod": "NODE_OPTIONS=--max_old_space_size=8000 NODE_ENV=production webpack --progress --color --watch",
    "webpack-measure": "COCALC_OUTPUT=dist-measure MEASURE=true NODE_ENV=development npm run webpack ",
    "webpack-measure-prod": "COCALC_OUTPUT=dist-prod-measure MEASURE=true npm run webpack-prod",
    "build": "./production-build.py"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sagemathinc/cocalc"
  },
  "keywords": [
    "python",
    "sage",
    "sagemath",
    "latex",
    "terminal",
    "web",
    "ide",
    "math",
    "mathematics"
  ],
